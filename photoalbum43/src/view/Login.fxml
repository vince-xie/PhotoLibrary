<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="loginPage" prefHeight="600.0" prefWidth="800.0" stylesheets="@Login.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">

   	<Text fx:id="name" layoutX="215.0" layoutY="250.0" text="Photo Album" />
   	<TextField layoutX="-300.0" layoutY="-300.0" prefHeight="0" prefWidth="0" />
    <TextField fx:id="Username" layoutX="306.0" layoutY="300.0" onMouseClicked="#resetUsername" prefHeight="45.0" prefWidth="190.0" promptText="Username" />  
    <Text fx:id="userwrong" layoutX="518.0" layoutY="327.0" text="User not found" />
    <PasswordField fx:id="Password" layoutX="306.0" layoutY="374.0" onMouseClicked="#resetPassword" prefHeight="45.0" prefWidth="190.0" promptText="Password" />    
    <Text fx:id="passwrong" layoutX="519.0" layoutY="401.0" text="Incorrect password" />     
    <Button fx:id="Login" layoutX="352.0" layoutY="450.0" onAction="#login" prefHeight="45.0" prefWidth="97.0" text="Login" />
   	
</AnchorPane>
